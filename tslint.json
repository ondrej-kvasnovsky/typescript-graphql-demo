{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "indent": [
      true,
      "spaces", 1
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "eofline": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  },
  "rulesDirectory": []
}
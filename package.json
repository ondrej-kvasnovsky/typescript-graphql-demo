{
  "name": "typescript-node-demo",
  "version": "1.0.0",
  "description": "",
  "main": "lib/src/index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:trace": "tsc -p . --traceResolution",
    "build:live": "nodemon --watch 'src/app/**/*.ts' --exec 'ts-node' src/app/index.ts",
    "lint": "tslint --fix -c ./tslint.json 'src/**/*.ts'",
    "test": "tsc && mocha --reporter spec --require ts-node/register src/**/*.spec.ts",
    "test:watch": "tsc && mocha -w --reporter spec --require ts-node/register src/**/*.spec.ts",
    "test:coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" mocha -r ts-node/register src/**/*.spec.ts && nyc report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "body-parser": "^1.20.1",
    "dialogflow": "^0.7.0",
    "express": "^4.18.2",
    "graphql": "^14.0.2",
    "inversify": "^5.0.1",
    "lodash": "^4.17.21",
    "pg": "^7.6.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.3.14"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.0.66",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-subset": "^1.3.1",
    "@types/dialogflow": "^0.6.3",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.3",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "@types/sinon": "^5.0.7",
    "@types/sinon-chai": "^3.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "mocha": "^10.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.1.6"
  }
}
